{
    "extends": ["eslint:recommended"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "parserOptions": { "ecmaVersion": 6 },
    "rules": {
        "eqeqeq": "error",
        "indent": ["error", 4, { "MemberExpression": 0 }],
        "quotes": ["warn", "single"]
    }
}
